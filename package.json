{
  "name": "Drch-Electron",
  "version": "1.2.0",
  "description": "DEMO: https://junauplayer.github.io",
  "main": "main.js",
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "HengiFettlich",
        "repo": "Drch-Electron",
        "releaseType": "release",
        "vPrefixedTagName": false
      }
    ],
    "appId": "drch.electron",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "author": "HengiFettlich",
  "license": "WTFPL",
  "devDependencies": {
    "electron": "^6.0.7",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-updater": "^4.1.2",
    "jquery": "^3.4.1",
    "ws": "^7.1.2"
  }
}
